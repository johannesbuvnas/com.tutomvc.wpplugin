{
  "name": "tutomvc",
  "version": "3.7.1",
  "description": "The WordPress MVC Framework for developers.",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/johannesbuvnas/tutomvc"
  },
  "scripts": {
    "postinstall": "",
    "clean:dist": "rm -r dist",
    "clean:phpdocs": "rm -r docs",
    "phpdocs": "vendor/bin/phpdoc -c phpdoc.dist.xml",
    "build:dev": "webpack --config src/node/webpack.dev.js --progress --profile --colors --display-error-details --display-cached --bail",
    "build:prod": "webpack --config src/node/webpack.prod.js --progress --profile --colors --display-error-details --display-cached --bail",
    "build": "npm run clean:dist && npm run build:dev && npm run build:prod",
    "watch:dev": "webpack --config src/node/webpack.dev.js --progress --profile --colors --display-error-details --display-cached -w"
  },
  "devDependencies": {
    "@types/backbone": "^1.3.42",
    "@types/bootstrap": "^3.3.38",
    "@types/core-js": "^0.9.46",
    "@types/jasmine": "^2.8.6",
    "@types/jquery": "^3.3.0",
    "@types/jquery-lazyload": "^1.9.1",
    "@types/node": "^9.4.6",
    "@types/tinymce": "^4.5.16",
    "@types/underscore": "^1.8.7",
    "bootstrap": "^3.3.6",
    "bootstrap-select": "^1.12.3",
    "clean-webpack-plugin": "^0.1.19",
    "css-loader": "^0.28.11",
    "css-to-string-loader": "^0.1.2",
    "file-loader": "^1.1.11",
    "glob": "^7.0.3",
    "jquery": "2.1.4",
    "jquery-ui": "1.12.1",
    "less": "^2.7.1",
    "less-loader": "^4.1.0",
    "loader-utils": "^1.1.0",
    "mime": "^1.3.4",
    "mini-css-extract-plugin": "^0.4.0",
    "minimist": "^1.2.0",
    "node-sass": "^4.9.0",
    "node-sass-utils": "^1.1.2",
    "optimize-css-assets-webpack-plugin": "^4.0.2",
    "sass-loader": "^7.0.2",
    "script-loader": "^0.7.0",
    "select2": "^4.0.3",
    "source-map-loader": "^0.2.3",
    "style-loader": "^0.21.0",
    "ts-loader": "^4.3.0",
    "typescript": "^2.9.1",
    "typings": "^1.0.4",
    "uglify-js": "^3.4.0",
    "uglify-loader": "^2.0.0",
    "uglifyjs-webpack-plugin": "^1.2.5",
    "underscore": "^1.8.3",
    "webpack": "^4.10.2",
    "webpack-cli": "^3.1.1",
    "webpack-fix-style-only-entries": "0.0.5",
    "webpack-glob-entry": "^2.1.1",
    "webpack-merge": "^4.1.2"
  }
